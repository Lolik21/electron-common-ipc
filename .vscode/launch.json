{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}"
        },
        {
            "name": "Launch Sandbox Test tr-apps",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "program": "${workspaceRoot}/examples/test-app/Main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}/examples/test-app/",
            "runtimeExecutable": "${workspaceRoot}/examples/test-app/node_modules/electron/dist/electron.exe",
            "runtimeArgs": [
                "--enable-sandbox",
                "--process-per-site",
                "--remote-debugging-port=8315"
            ],
            "env": {},
            "console": "internalConsole",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/**/*.js"
            ]
        },
        {
            "name": "Launch Standard Test tr-apps",
            "type": "node",
            "request": "launch",
            // "protocol": "legacy",
            "protocol": "inspector",
            "program": "${workspaceRoot}/examples/test-app/Main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}/examples/test-app/",
            "runtimeExecutable": "${workspaceRoot}/examples/test-app/node_modules/electron/dist/electron.exe",
            "runtimeArgs": [
                "--process-per-site",
                "--remote-debugging-port=8315"
            ],
            "env": {
                "NODE_PRESERVE_SYMLINKS": "1",
                "ELECTRON_IPC_BUS_LOGPATH": "C:\\temp\\electron_ipc_bus.log",
                "NODE_ENV": "development"
            },
            // "console": "integratedTerminal",
            "console": "internalConsole",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/**/*.js"
            ]
        }
    ]
}